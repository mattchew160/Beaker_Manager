{
    "manifest_version" : 3,
    "name": "Beaker Manger",
    "version": "1.0",
    "description": "This extentsion creates beaker popups and noises to mess with you while you work.",
    "icons" :{
        "128" : "icons/beaker128.png",
        "48":"icons/beaker48.png",
        "16":"icons/beaker16.png"

    },
    "storage" :{
        "managed_schema" : "schema.json"
    },

   
    "action": {
        "default_icon" : "icons/beaker16.png",
        "default_popup": "popup.html",
        "deafult_title" : "Beaker Manager"
    },


    "options_page" : "options.html",

    

    "permissions": [
        "storage",
        "tabs"
        

    ]








}
